//swaping of two numbers using temp variable
public class Student{
    public static void main(String[] args){
        int a = 10, b = 20;
        System.out.println("numbers before swaping a = "+a +" ,b = "+ b );
        
        int temp = a;
        a = b;
        b = temp;
        System.out.println("numbers after swaping " + "a = "+a +" b = "+b);
    }

}
